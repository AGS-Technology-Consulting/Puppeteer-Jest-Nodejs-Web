{
  "name": "puppeteer-pom-framework",
  "version": "1.0.0",
  "description": "Professional Puppeteer Automation Framework with Page Object Model",
  "main": "index.js",
  "scripts": {
    "test": "jest --detectOpenHandles --forceExit",
    "test:headed": "HEADLESS=false jest --detectOpenHandles --forceExit",
    "test:ci": "jest --ci --detectOpenHandles --forceExit",
    "allure:generate": "allure generate allure-results --clean -o allure-report",
    "allure:open": "allure open allure-report",
    "allure:serve": "allure serve allure-results",
    "clean": "rm -rf allure-results allure-report screenshots/*.png test-results"
  },
  "keywords": [
    "puppeteer",
    "automation",
    "testing",
    "page-object-model",
    "pom",
    "e2e"
  ],
  "author": "Pravin Gamit",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "allure-commandline": "^2.29.0",
    "jest": "^29.7.0",
    "jest-allure2-reporter": "^2.0.0-beta.11",
    "jest-html-reporter": "^3.10.2"
  },
  "dependencies": {
    "axios": "^1.6.7",
    "dotenv": "^16.4.5",
    "fs-extra": "^11.2.0",
    "puppeteer": "^22.0.0",
    "winston": "^3.11.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
